  doi           = {10.19571/j.cnki.1000-2995.2009.05.001},
  file          = {企业开放式创新的动态模式研究_陈钰芬.pdf:/Users/morvenliu/Zotero/storage/KGGD96JW/企业开放式创新的动态模式研究_陈钰芬.pdf:application/pdf},
  issn          = {1000-2995},
  journaltitle  = {科研管理},
  keywords      = {U-A模型, 创新要素, 动态模式, 开放式创新},
  langid        = {pinyin},
